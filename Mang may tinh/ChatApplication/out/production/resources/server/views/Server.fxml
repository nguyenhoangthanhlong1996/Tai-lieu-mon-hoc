<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="484.0" prefWidth="1049.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="server.controllers.ServerController">
    <children>
        <ToggleButton fx:id="toggleOnOffServer" layoutX="45.0" layoutY="14.0" mnemonicParsing="false"
                      onAction="#toggleOnOffServerClick" prefHeight="25.0" prefWidth="58.0" text="Bật"/>
        <ScrollPane fx:id="scrollPaneViewSysLog" hbarPolicy="NEVER" layoutX="21.0" layoutY="60.0" prefHeight="408.0"
                    prefWidth="753.0">
            <content>
                <VBox fx:id="vboxViewSysLog" maxWidth="750.0">
                    <padding>
                        <Insets left="5.0" right="10.0"/>
                    </padding>
                </VBox>
            </content>
            <padding>
                <Insets right="5.0"/>
            </padding>
        </ScrollPane>
        <TableView fx:id="tableConnect" layoutX="783.0" layoutY="42.0" prefHeight="423.0" prefWidth="248.0">
            <columns>
                <TableColumn fx:id="colPortID" prefWidth="123.0" text="Port"/>
                <TableColumn fx:id="colByUser" minWidth="6.0" prefWidth="124.0" text="Bởi username"/>
            </columns>
        </TableView>
        <Label layoutX="794.0" layoutY="16.0" prefHeight="21.0" prefWidth="159.0" text="Các kết nối hiện có">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
